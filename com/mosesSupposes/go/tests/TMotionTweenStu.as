package com.mosesSupposes.go.tests {	import org.goasap.events.GoEvent;	import org.goasap.items.GoItem;		//	import com.mosesSupposes.go.tests.GoTestBase;		import com.mosesSupposes.go.tutorials.MotionTweenStu;		/**	 * Use with Go Video Tutorial #1, "Build a Go tween class in 3 easy steps" 	 * available at <a href="http://go.mosessupposes.com/">the official Go Blog</a>.	 * 	 * @author Moses Gunesch	 */	public class TMotionTweenStu extends GoTestBase {				public function TMotionTweenStu() 		{			var m : MotionTweenStu = new MotionTweenStu(super.box(), 100, 100);			m.duration = 1.5;			m.useRounding = true;			m.useRelative = true;			m.addEventListener(GoEvent.COMPLETE, onMTweenEnd);						super.addButtonUI(m, .75);		}				private function onMTweenEnd(event : GoEvent=null) : void {			trace("Tween complete!");		}	}}